 int[,] mas = new int[10, 10];
            Random rnd = new Random();
            for(int i=0;i<10;i++)
            {
                for(int j=0;j<10;j++)
                {
                    mas[i, j] = rnd.Next();
                    Console.Write($"{ mas[i, j]} ");
                }
                Console.Write('\n');
            }
